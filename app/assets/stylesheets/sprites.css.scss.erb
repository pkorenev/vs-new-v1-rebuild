@mixin about-page-ic-48px{
  width:48px;
    height: 48px;
    background-size: 73% auto;

    margin-right: 0;

    &.search-ic{
      background-position: 0 -36px;
    }

    &.anal-ic{
      background-position: 0 10px;
    }

    &.met-ic{
      background-position: 0 -82px;
    }
}

.about-page-ic{
  position: absolute;
  left: -80px;
  display: inline-block;
  margin-top: -9px;
  margin-right: 20px;
  width: 65px;
  height: 65px;
  background-image: url(<%= asset_path("static_images/about-page-sprite.png") %>);
  background-position: 0px 0;
  background-repeat: no-repeat;
  vertical-align: text-top;
  line-height: 65px;

  @media(max-width:1600px){
    position: relative;
    left:0;
  }

  @media(max-width: 1100px) and (min-width:921px){
    @include about-page-ic-48px;

  }

  @media(max-width:350px){
    @include about-page-ic-48px;
  }

  @media(max-width:300px){
    display: none;
  }


 
}

@mixin about-page-ic-wrapper-65px{
  display: block;
    position: relative;
    width: 100%;

    .about-page-ic{
      background-position: 0 0;
      background-size: 100% auto;
      display: block;
      height: 65px;
      margin-left: auto;
      margin-right: auto;
      position: relative;
      width: 65px;

      &.anal-ic{
        background-position: 0px 0px;
        margin-top: -5px;
      }

      &.search-ic{
        background-position: 0px -89px;
      }

      &.met-ic{
        background-position: 0px -171px;
      }
    }
}

.about-page-ic-wrapper{
  @media(max-width:920px) and(min-width:750px){
    @include about-page-ic-wrapper-65px;
  }
}

