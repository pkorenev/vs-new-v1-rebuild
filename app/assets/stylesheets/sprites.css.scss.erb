.sprite-section-home{
  margin: 0;
  padding: 0;
  

  width:100%;
  position: relative;

  
  ul{
    padding:0;
    position: relative;
    right:0;
    width:100%;
    list-style: none;
    max-width: 600px;
    li{
      overflow: hidden;
      float: left;
      width: 8.333333333333332%; // 50/600*100
      margin-left: 4.166666666666666%; //  25/600*100
      text-align: center;
      position: relative;

      &:last-child{
        margin-right: 0;
      }

      a{
        display: block;
        width: 100%;

      }

      a>figure{
        max-height: 100%;
        max-width: 100%;
        height: 1px;
        padding-top: 100%;
        width: 100%;
        img{
          //width:800%;
          width: 50px;
          height: auto;
          position: absolute;
          margin-top:8px;
        }

        
      }
    }
  }
}

