


  <!DOCTYPE html>
  <html style="height:auto !imoportant;overflow:visible !important;" prefix="og: http://ogp.me/ns#" class="no-js <%= params[:controller] %>-controller <%= params[:action] %>-action" >
  <head role="main">

  	<%= render partial: "application/head_hreflang" %>

    <% if @static_page_data %>
        <title><%= @static_page_data.head_title %></title>
        <meta name="keywords" content="<%= @static_page_data.meta_keywords %>" />
        <meta name="description" content="<%= @static_page_data.meta_description %>" />
    <% else %>

    <% end %>


    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />

    <meta content="width=device-width, initial-scale=1.0" name="viewport" />

    <link rel="shortcut icon" href="<%= asset_path( 'static_images/favicon.ico' ) %>">

    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    "app" %>

    <script type="text/javascript">
        //jQuery('html').removeClass('no-js');
        var html_element = document.getElementsByTagName('html')[0];
        var list=html_element.classList;
        list.remove('no-js');
        html_element.attributes['class'] = list.toString();
    </script>

    <!--

     -->


    <!-- sticky footer fix for IE version <=6 and >=8 -->

    <!--[if !IE 7]>
    <style type="text/css">
        #wrap {display:table;height:100%}
    </style>
    <![endif]-->

    <!-- end -->

  </head>
  <body  role="content-holder" data-controller="<%= params[:controller] %>" data-action="<%= params[:action] %>" >
  <div id="wrapper">
    <div id="main">
      <%= render :partial => 'partials/menu' %>

      <% if params[:controller] == 'page' && params[:action] == 'index' %>
          <%= render :partial => 'partials/bjqs' %>
      <% end %>

      <% if params[:controller] == 'portfolio' && params[:action] == 'item' %>
          <%= render partial: 'partials/portfolio_banner' %>
      <% end %>

      <% if @untranslated_locale && !'nick-allows' %>
        <%= render partial: 'partials/locale_not_supported' %>
      <% end %>

      <div id="page-content">
        <%= yield %>
      </div>
    </div>

  </div>
  
  <% cache "#{I18n.locale}_footer" do %>
  <%= render :partial => 'partials/footer' %>
  </div>


  <%= render partial: 'partials/ga' %>
  <%# javascript_include_tag "application"  %>
  <%= javascript_include_tag "application",  'data-turbolinks-track' => true, 'data-turbolinks-eval' => false  %>
  <% end %>

  </body>
  </html>

