<div id="footer-wrapper">
  <footer class="footer-container">
      
      
      
      <div class="footer-contacts-wrapper">
        <div class="footer-contacts">
          <p id="phone"><span class="cont-sprite-s cont-s-mobile-icon"></span><a href="tel: +38 (050) 417-07-28" title="+38 (050) 417-07-28">+38 (050) 417-07-28</a></p>
          <p id="email"><span class="cont-sprite-s cont-s-email-icon"></span><a href="mailto: office@voroninstudio.eu" title="office@voroninstudio.eu">office@voroninstudio.eu</a></p>

        </div>
      </div>

      <div class="behance">
        <%= link_to image_tag('behance_footer.jpg'), '/behance', :title => 'Behance HOST' %>
      </div>
      <div class="get-me-top-wrapper">
        <figure class="get-me-top-container-holder">
          <div class="get-me-top-container">
            <div id="get-me-top">
              <%= image_tag("footer-get-top.jpg") %>
            </div>
        </div>
        </figure>
      </div>

      <div id="social-panel-and-copyrights">
