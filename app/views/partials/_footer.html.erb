            <li><a href="" title=""><span class="soc-buttons twitter-icon"></span></a></li>
            <li id="fb-like">
              <div id="fb-like-wrapper">
                <%= fc_connect %>
                <div class="fb-like" data-href="http://www.facebook.com/pages/Voronin-Design-Studio/327478650602139" data-send="false" data-layout="button_count" data-width="135" data-show-faces="false" data-font="verdana"></div>
              </div>
            </li>
          </ul>
        </div>
        <div class="copyrights-section">
          <%= show_copyrights  %>
        </div>
